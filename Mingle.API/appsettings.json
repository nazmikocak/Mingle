{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "secret": "${JWT_SECRET}",
    "issuer": "${JWT_ISSUER}",
    "audience": "${JWT_AUDIENCE}",
    "expiryInDays": 2
  },
  "Firebase": {
    "apiKey": "${FIREBASE_API_KEY}",
    "authDomain": "${FIREBASE_AUTH_DOMAIN}",
    "databaseUrl": "${FIREBASE_DATABASE_URL}"
  },
  "Cloudinary": {
    "cloudName": "${CLOUDINARY_CLOUD_NAME}",
    "apiKey": "${CLOUDINARY_API_KEY}",
    "apiSecret": "${CLOUDINARY_API_SECRET}"
  },
  "Gemini": {
    "apiKey": "${GEMINI_API_KEY}",
    "textUrl": "https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent?key="
  },
  "HuggingFace": {
    "apiKey": "${HUGGINGFACE_API_KEY}",
    "fluxImage": "https://router.huggingface.co/hf-inference/models/black-forest-labs/FLUX.1-dev",
    "artplesImage": "https://router.huggingface.co/hf-inference/models/Artples/LAI-ImageGeneration-vSDXL-2",
    "compvisImage": "https://router.huggingface.co/hf-inference/models/CompVis/stable-diffusion-v1-4"
  }
}